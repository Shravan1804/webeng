

<h1>Choose a photo</h1>
<%= form_tag(store_photo_url_activity_path, method: "post") do %>
<center>
<table>
	<% photos.each do |p| %>
		<tr>
			<td><%= radio_button_tag(:url, p) %></td>
			<td><%= label_tag(:url, image_tag(p)) %></td>
		</tr>
	<% end %>
</table>
<%= submit_tag("Submit") %>
</center>
<% end %>
<br/>
<%= link_to 'Back', :back %>